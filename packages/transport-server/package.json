{
  "name": "mongosh-transport-server",
  "version": "0.0.0",
  "description": "MongoDB Shell Transport Server Package",
  "main": "lib/index.js",
  "scripts": {
    "compile": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js --progress",
    "pretest": "cross-env NODE_ENV=test webpack --config ./config/webpack.test.config.js --progress",
    "test": "mocha --colors \"./src/**/*.spec.js\"",
    "test-ci": "mocha \"./src/**/*.spec.js\"",
    "check": "mongodb-js-precommit './src/**/*{.js}'",
    "prepublish": "npm run compile"
  },
  "dependency-check": {
    "entries": [
      "src/**/*.js"
    ],
    "ignore": [
      "sinon",
      "chai",
      "mongodb-runner"
    ]
  },
  "peerDependencies": {
    "mongodb": "^3.3.3",
    "mongodb-stitch-server-sdk": "^4.7.0"
  }
}
