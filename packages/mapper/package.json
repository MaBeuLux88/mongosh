    "mongosh-shell-api": "file:../shell-api",
    "pretty-bytes": "^5.3.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "mongosh-service-provider-server": "file:../service-provider-server",
    "ts-sinon": "^1.0.25"
  }
}
