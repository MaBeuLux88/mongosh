{
  "name": "mongosh-mapper",
  "version": "0.0.0",
  "description": "MongoDB Shell Mapper Package",
  "main": "./lib/index.js",
  "scripts": {
    "test": "mocha --colors -r ts-node/register \"./{src,lib}/**/*.spec.ts\"",
    "test-ci": "mocha -r ts-node/register \"./{src,lib}/**/*.spec.ts\"",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "check": "npm run lint",
    "compile-ts": "tsc -p tsconfig.json",
